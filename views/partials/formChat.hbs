<div class="container-fluid mt-3" id="chat">
  
  <h3>Centro de mensajes</h3>
  <div id="messages"></div>
  <div autocomplete="on">
    <label for="email">Email</label>
    <input
      type="email"
      id="email"
      class="form-control my-2 mr-2"
      placeholder="Ingresa tu email"
      required
    />
    <label for="nombre">Nombre</label>
    <input
      type="text"
      id="nombre"
      class="form-control my-2 mr-2"
      placeholder="Ingresa tu nombre"
      required
    />
    <label for="apellido">Apellido</label>
    <input
      type="text"
      id="apellido"
      class="form-control my-2 mr-2"
      placeholder="Ingresa tu apellido"
      required
    />
    <label for="edad">Edad</label>
    <input
      type="number"
      id="edad"
      class="form-control my-2 mr-2"
      placeholder="Ingresa tu edad"
      required
    />
    <label for="alias">Alias</label>
    <input
      type="text"
      id="alias"
      class="form-control my-2 mr-2"
      placeholder="Ingresa tu alias"
      required
    />
    <label for="avatar">Avatar</label>
    <input
      type="url"
      id="avatar"
      class="form-control my-2 mr-2"
      placeholder="Ingresa tu avatar"
      required
    />
    <label for="message">Mensaje</label>
    <input
      type="text"
      id="message"
      class="form-control my-2 mr-2"
      placeholder="CuÃ©ntanos algo..."
      required
    />
    <button class="btn btn-dark mt-3" id="buttonChat"> Enviar!</button>

  
</div>